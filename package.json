{
	"name": "semantic-comments",
	"displayName": "semantic-comments",
	"description": "",
	"publisher": "test",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Snippets"
	],
	"contributes": {
		"configuration": {
			"title": "Semantic Comments",
			"properties": {
				"Toggle suggestions": {
					"type": "boolean",
					"scope": "resource",
					"default": true,
					"description": "Enable auto suggestions"
				}
			}
		},
		"snippets": [
			{
				"language": "html",
				"path": "./snippets/snippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
